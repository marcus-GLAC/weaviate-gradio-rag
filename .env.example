# Rename this file to .env and add your API keys
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Model settings
DEFAULT_LLM_PROVIDER=gemini  # gemini or openai

# OpenAI model settings
OPENAI_MODEL=gpt-3.5-turbo  # gpt-3.5-turbo, gpt-4, gpt-4-turbo
OPENAI_TEMPERATURE=0

# Gemini model settings
GEMINI_MODEL=gemini-pro  # gemini-pro, gemini-pro-vision, gemini-1.5-pro
GEMINI_TEMPERATURE=0
